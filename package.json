{
  "name": "react-trailer",
  "version": "1.1.0",
  "description": "Video Player for React",
  "main": "build/react-trailer.js",
  "scripts": {
    "build": "webpack --config ./scripts/webpack.config.js --progress --colors --profile --display-modules",
    "lint": "eslint ./src/**/*.js",
    "prerelease": "git checkout master && git pull origin master && npm i && git-authors-cli && git add .",
    "release": "standard-version -a",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Naimikan/react-trailer.git"
  },
  "keywords": [
    "react",
    "video",
    "player",
    "html5"
  ],
  "author": "Guillem Aldeguer Lima <guillem.aldeguer@gmail.com> (https://naimikan.github.io/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Naimikan/react-trailer/issues"
  },
  "homepage": "https://github.com/Naimikan/react-trailer#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "conventional-github-releaser": "^3.1.3",
    "copy-webpack-plugin": "^5.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^4.3.0",
    "git-authors-cli": "^1.0.26",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^10.2.6",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "standard-version": "^8.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "styled-components": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Naimikan Kimn <guillem.aldeguer@gmail.com>"
  ]
}
